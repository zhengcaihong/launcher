<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Widget.Ali.Button">
        <item name="android:background">@drawable/hw_btn_default</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:textAppearance">@style/TextAppearance.Ali.Widget.Button</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <!--
        button text
        <item name="android:textSize">@dimen/hw_button_text_size</item>
        -->
    </style>

    <style name="Widget.Ali.Button.Small">
        <item name="android:minHeight">25dp</item>
        <item name="android:background">@drawable/hw_btn_default_small</item>
        <item name="android:textAppearance">@style/TextAppearance.Ali.Widget.Button.Small</item>
    </style>

    <style name="Widget.Ali.Button.Inset" parent="android:style/Widget.Holo.Button.Inset">
    </style>

    <style name="Widget.Ali.Button.Toggle" parent="@android:style/Widget.Holo.Button.Toggle">
        <item name="android:textAppearance">@style/TextAppearance.Ali.Small</item>
    </style>

    <style name="Widget.Ali.Button.Dialog">
	<item name="android:background">@drawable/hw_alert_dialog_btn_bar_bg</item>
        <item name="android:minHeight">@dimen/hw_alert_dialog_button_bar_height</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>
    <style name="Theme.Ali.Dialog.ActionSheet" parent="Theme.Ali.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/Animation.Translucent.Actionsheet</item>
    </style>

    <style name="Animation.Translucent.Actionsheet" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/hw_actionsheet_enter</item>
        <item name="android:windowExitAnimation">@anim/hw_actionsheet_exit</item>
    </style>
<style name="Widget.Ali.Button.Dialog.Left">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>

    <style name="Widget.Ali.Button.Dialog.Middle">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>

         
    <style name="Widget.Ali.Button.Dialog.Right">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_ok_button_text_color</item>
