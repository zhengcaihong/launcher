<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/hw_scrubber_control_disabled" android:gravity="center" android:tint="?attr/hw_colorPrimaryDisabledInner"/>
    </item>
    <item android:state_pressed="true" android:id="@+id/pressed">
        <bitmap android:src="@drawable/hw_scrubber_control_press_09" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
    </item>
    <item android:id="@+id/not_pressed">
        <bitmap android:src="@drawable/hw_scrubber_control_press_01" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
    </item>
    <transition android:fromId="@+id/not_pressed" android:toId="@+id/pressed">
        <animation-list>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_01" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_02" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_03" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_04" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_05" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
               <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_06" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
               <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_07" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
               <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_08" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
               <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_press_09" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
        </animation-list>
    </transition>
    <transition android:fromId="@+id/pressed" android:toId="@+id/not_pressed">
        <animation-list>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_01" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_02" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_03" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_04" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_05" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
            <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_06" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
             <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_07" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
             <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_08" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
             <item android:duration="30">
                <bitmap android:src="@drawable/hw_scrubber_control_release_09" android:gravity="center" android:tint="?attr/hw_colorPrimary" />
            </item>
        </animation-list>
    </transition>
</animated-selector>
