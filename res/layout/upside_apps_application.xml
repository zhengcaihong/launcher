<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
    <com.aliyun.homeshell.icon.BubbleTextView
        android:id="@+id/title"
        android:layout_marginTop="@dimen/folder_app_select_icon_offset"
        android:layout_marginRight="@dimen/folder_app_select_icon_offset"
        style="@style/WorkspaceIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:background="@color/transparent"/>
    <ImageView
        android:id="@+id/selector"
        android:layout_width="@dimen/folder_app_select_selector_width"
        android:layout_height="@dimen/folder_app_select_selector_height"
        android:layout_gravity="right|top"/>
</FrameLayout>