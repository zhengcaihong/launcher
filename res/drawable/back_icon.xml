<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false"><bitmap android:src="@drawable/hw_actionbar_back_disabled" android:tint="@color/back_color_disabled" />
    </item>
    <item android:state_pressed="true"><bitmap android:src="@drawable/hw_actionbar_back_pressed" android:tint="@color/back_color_dark" />
    </item>
    <item android:state_focused="true"><bitmap android:src="@drawable/hw_actionbar_back_normal" android:tint="@color/back_color" />
    </item>
    <item><bitmap android:src="@drawable/hw_actionbar_back_normal" android:tint="@color/back_color" />
    </item>

</selector>